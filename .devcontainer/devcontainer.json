{
	"name": "Petclinic",
	"dockerFile": "Dockerfile",
	"runArgs": [
		"--cap-add=SYS_PTRACE",
		"--security-opt",
		"seccomp=unconfined"
	],
	"remoteUser": "vscode",
	"features": {
		"docker-in-docker": "latest"
	},
	"extensions": [
		"vscjava.vscode-java-pack",
		"redhat.vscode-xml",
		"vmware.vscode-boot-dev-pack",
		"mhutchie.git-graph"
	],
	"forwardPorts": [8080],
	"settings": {
		"java.import.gradle.enabled": false,
		"java.server.launchMode": "Standard"
	}
}
